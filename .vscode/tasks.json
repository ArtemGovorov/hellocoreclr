{
	"version": "0.1.0",
	"command": "sh",
	"args": ["-c"],
	"isShellCommand": true,
	"showOutput": "always",
	"suppressTaskName": true,
	"tasks": [
		{
			"taskName": "Build All",
			"args": ["dotnet build ${workspaceRoot};${workspaceRoot}/ui/node_modules/.bin/tsc -p ${workspaceRoot}/ui"],
			"isBuildCommand": true
		},
		{
			"taskName": "Compile Typescript/tsc",
			"args": ["${workspaceRoot}/ui/node_modules/.bin/tsc -p ${workspaceRoot}/ui"],
			"problemMatcher": "$tsc"
		},
		{
			"taskName": "Build C#/dotnet",
			"args": ["dotnet build ${workspaceRoot}"],
			"problemMatcher": "$msCompile"
		}
	]
}
