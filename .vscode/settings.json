// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,

    "ui/src/**/*.js": { "when": "$(basename).ts"},
    "ui/src/**/*.js.map": true,
    "ui/src/app/appsettings.js": true,
    "ui/test/**/*.js": { "when": "$(basename).ts"},
    "ui/test/**/*.js.map": true,
    "ui/wwwroot/**": true,
    "ui/node_modules/**": true,
    "ui/bower_components/**": true,
    "ui/typings/**": true,
    "ui/project.lock.json": true,
    "src/bin/**": true,
    "src/obj/**": true,
    "src/project.lock.json": true,
    "test/bin/**": true,
    "test/obj/**": true,
    "test/project.lock.json": true,
    ".vscode/**": true,
    "node_modules/**": true,
    "dist/**": true
  },
  "stylelint.config": {
    "extends": "./ui/stylelint.config.js" 
  }
}
