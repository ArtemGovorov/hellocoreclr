// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,

    "HelloCoreClrApp.Ui/src/**/*.js": { "when": "$(basename).ts"},
    "HelloCoreClrApp.Ui/src/**/*.js.map": true,
    "HelloCoreClrApp.Ui/src/app/appsettings.js": true,
    "HelloCoreClrApp.Ui/test/**/*.js": { "when": "$(basename).ts"},
    "HelloCoreClrApp.Ui/test/**/*.js.map": true,
    "HelloCoreClrApp.Ui/wwwroot/**": true,
    "HelloCoreClrApp.Ui/node_modules/**": true,
    "HelloCoreClrApp.Ui/bower_components/**": true,
    "HelloCoreClrApp.Ui/typings/**": true,
    "HelloCoreClrApp.Ui/bin/**": true,
    "HelloCoreClrApp.Ui/project.lock.json": true,
    "HelloCoreClrApp/bin/**": true,
    "HelloCoreClrApp/project.lock.json": true,
    "HelloCoreClrApp.Test/bin/**": true,
    "HelloCoreClrApp.Test/project.lock.json": true,
    ".vscode/**": true,
    "node_modules/**": true,
    "dist/**": true
  },
  "stylelint.config": {
    "extends": "./HelloCoreClrApp.Ui/stylelint.config.js" 
  }
}
