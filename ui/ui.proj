  <ItemGroup>
	  <Artifacts Include="wwwroot/**"/>
  </ItemGroup>

  <Target Name="Publish">
    <RemoveDir Directories="$(output)" /> 
    <Copy SourceFiles="@(Artifacts)"     
      DestinationFiles="@(Artifacts->'$(output)/%(RecursiveDir)%(Filename)%(Extension)')"/>
  </Target>

</Project>
