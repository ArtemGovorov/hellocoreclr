      "imports": [
        "portable-net45"
      ]
    }
  },

  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-020000",
      "imports": [
        "dnxcore50",
        "portable-net45",
        "netstandard1.3"
      ]
    }
  }
}
